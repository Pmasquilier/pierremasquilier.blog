// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

// schema.prisma
datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Article {
  id        String @id @default(cuid())
  name      String
  categorie String
  title     String
  subtitle  String
  date      String
}
